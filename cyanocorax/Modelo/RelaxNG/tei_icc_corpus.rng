         <ref name="ICCatt.pointing.attributes"/>
         <ref name="ICCatt.internetMedia.attributes"/>
         <ref name="ICCatt.typed.attributes"/>
         <ref name="ICCatt.declaring.attributes"/>
         <ref name="ICCatt.cReferencing.attributes"/>
