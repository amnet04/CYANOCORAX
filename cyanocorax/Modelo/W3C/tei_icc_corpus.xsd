                <xs:union memberTypes="ns1:ICCdata.word ns1:ICCdata.word ns1:ICCdata.word"/>
              </xs:simpleType>
            </xs:list>
          </xs:simpleType>
          <xs:minLength value="2"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
