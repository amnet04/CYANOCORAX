      </xs:annotation>
      <xs:simpleType>
        <xs:restriction>
          <xs:simpleType>
            <xs:list>
              <xs:simpleType>
                <xs:union memberTypes="ns1:ICCdata.pointer ns1:ICCdata.pointer ns1:ICCdata.pointer"/>
              </xs:simpleType>
            </xs:list>
          </xs:simpleType>
          <xs:minLength value="2"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="ICCatt.pointing.group.attribute.targFunc">
    <xs:attribute name="targFunc">
      <xs:annotation>
        <xs:documentation>(función meta) describe la función de cada uno de los valores del atributo targets (metas) de los marcadores correspondientes link, join, o alt.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction>
          <xs:simpleType>
            <xs:list>
